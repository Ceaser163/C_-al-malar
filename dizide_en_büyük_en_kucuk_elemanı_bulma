#include <stdio.h>
#include <stdlib.h>

int main()
{
      int i,j,k,max,min;
      int sayilar[4];
      
      for(i=0;i<5;i++)
      {
	  printf("sayilari giriniz: ");
      scanf("%d",&sayilar[i]);
      }
      min=sayilar[0];
      max=sayilar[0];
      for(j=0;j<4;++j)
      {
       	if(max<sayilar[j])
	  {
      	  max=sayilar[j];
	}
    }
      for(k=0;k<4;++k)
	{
      	if(min>sayilar[k])
	{
      	min=sayilar[k];	
	}
    }
      
      printf("en buyuk sayi: %d/n",max);
      printf("en kucuk sayi: %d",min);
      return 0;
}
